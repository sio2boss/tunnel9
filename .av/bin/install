#! /bin/bash


# Import color
source $AV_CONFIG_DIR/default_imports
source $AV_CONFIG_DIR/findjar

# Check for a help flag
avCheckHelpFlags "$1" "tunnel9" "install tunnel9 for the local system" "\n \
    Template to help you out.\n \
\n"

# Import system path

#### Put system commands here
avAppendSystemPath

cd $AV_PROJ_TOP && cp ./tunnel9 ~/.local/bin/
echo "tunnel9 installed to ~/.local/bin/tunnel9"

#### End commands
